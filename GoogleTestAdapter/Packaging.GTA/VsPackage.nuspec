    <file src="GoogleTestAdapter.Common.Dynamic.dll" target="build\_common\" />
    <file src="GoogleTestAdapter.Core.dll" target="build\_common\" />
    <file src="GoogleTestAdapter.DiaResolver.dll" target="build\_common\" />
