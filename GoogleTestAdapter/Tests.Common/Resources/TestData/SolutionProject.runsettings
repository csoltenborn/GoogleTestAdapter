<RunSettings>
  <GoogleTestAdapterSettings>
    <SolutionSettings>
      <Settings>
        <ShuffleTests>true</ShuffleTests>
        <MaxNrOfThreads>3</MaxNrOfThreads>
        <ShuffleTestsSeed>3</ShuffleTestsSeed>
        <WorkingDir>$(ExecutableDir)\..</WorkingDir>
        <TestNameSeparator>!</TestNameSeparator>
	    <EnvironmentVariables>MYENVVAR=MyValue</EnvironmentVariables>
      </Settings>
    </SolutionSettings>
    <ProjectSettings>
      <Settings ProjectRegex=".*\\Tests_gta\.exe">
        <TestNameSeparator>|</TestNameSeparator>
      </Settings>
    </ProjectSettings>
  </GoogleTestAdapterSettings>
</RunSettings>